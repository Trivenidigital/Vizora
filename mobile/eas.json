{
  "cli": {
    "version": ">= 13.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://localhost:3000",
        "EXPO_PUBLIC_REALTIME_URL": "http://localhost:3002"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.vizora.cloud",
        "EXPO_PUBLIC_REALTIME_URL": "https://realtime.vizora.cloud"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.vizora.cloud",
        "EXPO_PUBLIC_REALTIME_URL": "https://realtime.vizora.cloud"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
