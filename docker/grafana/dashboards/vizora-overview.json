        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["lastNotNull", "max"]
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "editorMode": "code",
          "expr": "(\n  sum(rate(vizora_http_errors_total[5m]))\n  /\n  sum(rate(vizora_http_requests_total[5m]))\n) * 100",
          "legendFormat": "Error Rate",
          "range": true,
          "refId": "A"
        }
      ]
    },
