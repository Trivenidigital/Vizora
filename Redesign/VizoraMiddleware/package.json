{
  "name": "vizora-middleware",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node --max-old-space-size=6144 src/server.js",
    "start-debug": "node --max-old-space-size=4096 --expose-gc src/server.js",
    "dev-memory": "nodemon --max-old-space-size=6144 --expose-gc src/server.js",
    "seed": "node src/seed/userSeed.js",
    "test:api": "node test-api.js",
    "build": "echo Building server...",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "create-user": "node create-test-user.js",
    "clean": "rimraf dist",
    "prepare": "npm run clean ; npm run build",
    "postinstall": "node -e \"try { require('fs').mkdirSync('./logs') } catch (e) { if (e.code !== 'EEXIST') throw e }\""
  },
  "keywords": [
    "vizora",
    "digital signage",
    "middleware",
    "display management",
    "content management"
  ],
  "author": "Vizora Team",
  "license": "MIT",
  "dependencies": {
    "@socket.io/redis-adapter": "^8.2.1",
    "@vizora/common": "file:../common",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb-memory-server": "^9.1.6",
    "mongoose": "^8.1.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "node-fetch": "^2.7.0",
    "qrcode": "^1.5.4",
    "react-dropzone": "^14.3.8",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.1.1",
    "cors": "^2.8.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "msw": "^2.7.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.8.2",
    "vite": "^5.1.0",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
